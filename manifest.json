{
  "name": "Synergy Addons",
  "version": "1.0",
    "content_scripts": [
  {
    "matches": [
      "https://wa-bsd405-psv.edupoint.com/*"
    ],
      
    "js": ["content.js", "studentid.js"]
  }
],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "popup.html"
    }, 
    "default_icon": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "permissions": ["activeTab", "declarativeContent", "storage"],
  "description": "Lots of modifications to Synergy StudentVUE!",
  "manifest_version": 2
}
